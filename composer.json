{
    "name": "martin-ro/blade-alpine-instantsearch",
    "description": "Algolia instant search as Blade/Alpine.js components",
    "keywords": [
        "laravel",
        "algolia",
        "instantsearch"
    ],
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MartinRo\\BladeInstantSearch\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MartinRo\\BladeInstantSearch\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
            "providers": [
                "MartinRo\\BladeInstantSearch\\Providers\\BladeInstantSearchProvider"
            ]
        }
    },
    "require": {
        "php": ">=8.0",
        "illuminate/view": "^8.71|^9.0|dev-master",
        "illuminate/support": "^8.71|^9.0|dev-master"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.0"
    },
    "archive": {
        "exclude": [
            "/helpers",
            "/tests",
            "/js/src",
            "/js/package.json",
            "/js/yarn.lock"
        ]
    }
}
